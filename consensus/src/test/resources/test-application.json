{
  "server": {
    "port": 7070
  },
  "higgs.trust": {
    "nodeName": "TRUST-myNode",
    "masterName": "TRUST-masterNode",
    "privateKey": "abc",
    "prefix": "TRUST"
  },
  "TRUST": {
    "ribbon.NFLoadBalancerRuleClassName": "com.higgs.trust.common.feign.RequestHeaderRoundRobinRule"
  },
  "eureka": {
    "client": {
      "serviceUrl": {
        "defaultZone": "http://localhost:8761/eureka/"
      }
    }
  },
  "spring": {
    "application": {
      "name": "TRUST-myNode"
    },
    "datasource": {
      "druid": {
        "driver-class-name": "com.mysql.jdbc.Driver",
        "initialSize": 1,
        "maxActive": 10,
        "maxWait": 60000,
        "min-evictable-idle-time-millis": 300000,
        "minIdle": 1,
        "password": "123456",
        "time-between-eviction-runs-millis": 100000,
        "url": "jdbc:mysql://192.168.56.3:3306/trust?useUnicode=true&characterEncoding=UTF8&allowMultiQueries=true&useAffectedRows=true",
        "username": "root"
      },
      "type": "com.alibaba.druid.pool.DruidDataSource"
    }
  },
  "copycat": {
    "server": {
      "address": "127.0.0.1:9800",
      "cluster": "127.0.0.1:9800",
      "stateMachineClass": "com.higgs.trust.consensus.bft.example.StringStateMachine",
      "logDir": "D:/temp/raft"
    },
    "client": {
      "server": "127.0.0.1:9800"
    }
  },
  "mybatis": {
    "configLocation": "classpath:mybatis-config.xml",
    "mapper-locations": "classpath:mybatis/**/*Mapper.xml"
  },
  "consensus": {
    "p2p": {
      "data": {
        "dir": "./data/p2p"
      }
    }
  }
}