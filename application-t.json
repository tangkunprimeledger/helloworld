{
  "sign.pub.key": "test",
  "eureka": {
    "client": {
      "serviceUrl": {
        "defaultZone": "http://localhost:8761/eureka/"
      }
    }
  },
  "logging": {
    "config": "classpath:logback.xml"
  },
  "management": {
    "port": 6060,
    "security": {
      "enabled": false
    }
  },
  "mybatis": {
    "configLocation": "classpath:mybatis-config.xml",
    "mapper-locations": "classpath:mybatis/*Mapper.xml"
  },
  "nodeId": 1,
  "server": {
    "port": 7070
  },
  "spring": {
    "application": {
      "name": "higgs.trust"
    },
    "datasource": {
      "druid": {
        "driver-class-name": "com.mysql.jdbc.Driver",
        "initial-size": 1,
        "max-active": 20,
        "max-wait": 60000,
        "min-evictable-idle-time-millis": 300000,
        "min-idle": 1,
        "password": "",
        "time-between-eviction-runs-millis": 60000,
        "url": "jdbc:mysql://localhost:3306/trust?useUnicode=true&characterEncoding=UTF8",
        "username": "root"
      },
      "type": "com.alibaba.druid.pool.DruidDataSource"
    },
    "http": {
      "encoding": {
        "charset": "UTF-8",
        "enabled": true,
        "force": true
      }
    }
  }
}